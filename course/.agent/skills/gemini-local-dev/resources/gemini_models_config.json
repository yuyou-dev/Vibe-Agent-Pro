      "resolution_levels": {
        "low": {
          "image_tokens": 280,
          "video_tokens": 70
        },
        "medium": {
          "image_tokens": 560,
          "video_tokens": 70
        },
        "high": {
          "image_tokens": 1120,
          "video_tokens": 280
