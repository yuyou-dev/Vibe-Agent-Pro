    "gemini-2.5-flash-preview-tts": {
      "name": "Gemini 2.5 Flash Preview TTS",
      "category": "text_to_speech",
      "description": "语音合成模型，支持单说话人和多说话人模式",
      "api_version": "v1beta",
      "endpoint": "generateContent",
      "default_params": {
        "responseModalities": [
          "AUDIO"
        ]
      },
      "request_template_single": {
        "contents": [
          {
            "parts": [
              {
                "text": "{{text}}"
              }
            ]
          }
        ],
        "generationConfig": {
          "responseModalities": [
            "AUDIO"
          ],
          "speechConfig": {
            "voiceConfig": {
              "prebuiltVoiceConfig": {
                "voiceName": "{{voice_name}}"
              }
            }
          }
        }
      },
      "request_template_multi": {
        "contents": [
          {
            "parts": [
              {
                "text": "{{conversation}}"
              }
            ]
          }
        ],
        "generationConfig": {
          "responseModalities": [
            "AUDIO"
          ],
          "speechConfig": {
            "multiSpeakerVoiceConfig": {
              "speakerVoiceConfigs": [
                {
                  "speaker": "{{speaker1}}",
                  "voiceConfig": {
                    "prebuiltVoiceConfig": {
                      "voiceName": "{{voice1}}"
                    }
                  }
                },
                {
                  "speaker": "{{speaker2}}",
                  "voiceConfig": {
                    "prebuiltVoiceConfig": {
                      "voiceName": "{{voice2}}"
                    }
                  }
                }
              ]
            }
          }
        }
      },
      "voices": [
        {
          "name": "Kore",
          "style": "Firm"
        },
        {
          "name": "Puck",
          "style": "Upbeat"
        },
        {
          "name": "Zephyr",
          "style": "Bright"
        },
        {
          "name": "Charon",
          "style": "Informative"
        },
        {
          "name": "Fenrir",
          "style": "Excitable"
        },
        {
          "name": "Leda",
          "style": "Youthful"
        }
      ],
      "response_example": {
        "candidates": [
          {
            "content": {
              "parts": [
                {
                  "inlineData": {
                    "data": "BASE64_PCM_AUDIO_DATA..."
                  }
                }
              ]
            }
          }
        ]
      },
