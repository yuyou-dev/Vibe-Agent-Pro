        ],
        "usageMetadata": {
          "promptTokenCount": 280,
          "candidatesTokenCount": 100,
          "totalTokenCount": 380
        }
      },
      "extract_path": "candidates[0].content.parts[0].text",
      "use_cases": [
        "图片理解",
        "音频转录",
        "视频分析",
        "文档理解"
      ],
      "keywords": [
        "image",
