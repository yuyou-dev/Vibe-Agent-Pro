      "request_template_reasoning": {
        "contents": [
          {
            "parts": [
              {
                "text": "{{prompt}}"
              }
            ]
          }
        ],
        "generationConfig": {
          "thinkingConfig": {
            "thinkingLevel": "{{thinking_level}}"
          }
        }
      },
      "request_template_structured": {
        "contents": [
          {
            "parts": [
              {
                "text": "{{prompt}}"
              }
            ]
          }
        ],
        "generationConfig": {
          "responseMimeType": "application/json",
          "responseJsonSchema": "{{json_schema}}"
        },
        "tools": [
          {
            "googleSearch": {}
          }
        ]
      },
      "request_template_media_resolution": {
        "contents": [
