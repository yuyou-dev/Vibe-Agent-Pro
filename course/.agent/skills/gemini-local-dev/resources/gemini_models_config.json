{
  "api_base": {
    "v1beta": "https://generativelanguage.googleapis.com/v1beta",
    "v1alpha": "https://generativelanguage.googleapis.com/v1alpha",
    "auth_header": "x-goog-api-key",
    "env_key": "GEMINI_API_KEY"
  },
  "models": {
    "gemini-2.0-flash-exp": {
      "name": "Gemini 2.0 Flash Experimental",
      "category": "text_generation",
      "description": "快速文本生成模型，适用于对话、写作、翻译等",
      "api_version": "v1beta",
      "endpoint": "generateContent",
      "default_params": {
        "temperature": 0.7,
        "maxOutputTokens": 1024,
        "topP": 0.9,
        "topK": 40
      },
      "request_template": {
        "contents": [
          {
            "parts": [
              {
                "text": "{{prompt}}"
              }
            ]
          }
        ],
        "generationConfig": {
          "temperature": "{{temperature}}",
          "maxOutputTokens": "{{maxOutputTokens}}"
        }
      },
      "response_example": {
        "candidates": [
          {
            "content": {
              "parts": [
                {
                  "text": "生成的文本内容..."
                }
              ],
              "role": "model"
            },
            "finishReason": "STOP",
            "avgLogprobs": -0.1
          }
        ],
        "usageMetadata": {
          "promptTokenCount": 10,
          "candidatesTokenCount": 50,
          "totalTokenCount": 60
        }
      },
      "extract_path": "candidates[0].content.parts[0].text",
      "use_cases": [
        "创意写作",
        "内容摘要",
        "文本翻译",
        "问答系统",
        "代码生成"
      ],
      "keywords": [
        "text",
        "chat",
        "conversation",
        "write",
        "generate"
      ]
    },
    "gemini-2.5-flash": {
      "name": "Gemini 2.5 Flash",
      "category": "multimodal_understanding",
      "description": "多模态理解模型，支持图片、音频、视频、文档理解",
      "api_version": "v1beta",
      "endpoint": "generateContent",
      "supported_media": {
        "image": [
          "image/jpeg",
          "image/png",
          "image/webp",
          "image/gif"
